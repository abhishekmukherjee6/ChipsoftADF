{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ChipsoftADF"
		},
		"lnkSnowflake_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'lnkSnowflake'"
		},
		"linkBlob_sasUri": {
			"type": "secureString",
			"metadata": "Secure string for 'sasUri' of 'linkBlob'"
		},
		"lnkSnowflake_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "GLOBALTENDER"
		},
		"lnkSnowflake_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "vt90872.central-india.azure.snowflakecomputing.com"
		},
		"US_properties_ProcessTender_parameters_Country": {
			"type": "string",
			"defaultValue": "US"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "[concat(parameters('factoryName'), '/ProcessTender')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "Copy Tender Data",
						"type": "Copy",
						"dependsOn": [
							{
								"activity": "Set File Name",
								"dependencyConditions": [
									"Succeeded"
								]
							}
						],
						"policy": {
							"timeout": "0.12:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"source": {
								"type": "DelimitedTextSource",
								"storeSettings": {
									"type": "AzureBlobStorageReadSettings",
									"recursive": true,
									"enablePartitionDiscovery": false
								},
								"formatSettings": {
									"type": "DelimitedTextReadSettings"
								}
							},
							"sink": {
								"type": "SnowflakeV2Sink",
								"importSettings": {
									"type": "SnowflakeImportCopyCommand"
								}
							},
							"enableStaging": true,
							"stagingSettings": {
								"linkedServiceName": {
									"referenceName": "linkBlob",
									"type": "LinkedServiceReference"
								},
								"path": "chipsoftblob",
								"enableCompression": true
							}
						},
						"inputs": [
							{
								"referenceName": "DS_Tender",
								"type": "DatasetReference",
								"parameters": {
									"FolderName": {
										"value": "@concat(pipeline().parameters.Country,'/inbox')",
										"type": "Expression"
									},
									"FileName": {
										"value": "@variables('FileName')",
										"type": "Expression"
									}
								}
							}
						],
						"outputs": [
							{
								"referenceName": "SF",
								"type": "DatasetReference",
								"parameters": {}
							}
						]
					},
					{
						"name": "Set Current Date",
						"type": "SetVariable",
						"dependsOn": [],
						"policy": {
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"variableName": "CurrentDate",
							"value": {
								"value": "@formatDateTime(utcNow(),'MMddyyyy')",
								"type": "Expression"
							}
						}
					},
					{
						"name": "Set File Name",
						"type": "SetVariable",
						"dependsOn": [
							{
								"activity": "Set Current Date",
								"dependencyConditions": [
									"Succeeded"
								]
							}
						],
						"policy": {
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"variableName": "FileName",
							"value": {
								"value": "@concat(variables('CurrentDate'),'.csv') ",
								"type": "Expression"
							}
						}
					}
				],
				"policy": {
					"elapsedTimeMetric": {}
				},
				"parameters": {
					"Country": {
						"type": "string"
					}
				},
				"variables": {
					"CurrentDate": {
						"type": "String"
					},
					"FileName": {
						"type": "String"
					}
				},
				"annotations": []
			},
			"dependsOn": [
				"[concat(variables('factoryId'), '/datasets/DS_Tender')]",
				"[concat(variables('factoryId'), '/datasets/SF')]",
				"[concat(variables('factoryId'), '/linkedServices/linkBlob')]"
			]
		},
		{
			"name": "[concat(parameters('factoryName'), '/copy file')]",
			"type": "Microsoft.DataFactory/factories/pipelines",
			"apiVersion": "2018-06-01",
			"properties": {
				"activities": [
					{
						"name": "copy csv file",
