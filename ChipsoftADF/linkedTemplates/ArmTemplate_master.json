{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ChipsoftADF"
		},
		"lnkSnowflake_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'lnkSnowflake'"
		},
		"linkBlob_sasUri": {
			"type": "secureString",
			"metadata": "Secure string for 'sasUri' of 'linkBlob'"
		},
		"lnkSnowflake_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "GLOBALTENDER"
		},
		"lnkSnowflake_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "vt90872.central-india.azure.snowflakecomputing.com"
		},
		"USA_properties_ProcessTender_parameters_Country": {
			"type": "string",
			"defaultValue": "USA"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ChipsoftADF_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"lnkSnowflake_password": {
						"value": "[parameters('lnkSnowflake_password')]"
					},
					"linkBlob_sasUri": {
						"value": "[parameters('linkBlob_sasUri')]"
					},
					"lnkSnowflake_properties_typeProperties_database": {
						"value": "[parameters('lnkSnowflake_properties_typeProperties_database')]"
					},
					"lnkSnowflake_properties_typeProperties_host": {
						"value": "[parameters('lnkSnowflake_properties_typeProperties_host')]"
					},
					"USA_properties_ProcessTender_parameters_Country": {
						"value": "[parameters('USA_properties_ProcessTender_parameters_Country')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}